    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DevsGroupPolicy",
            "Effect": "Allow",
            "Action": [
                "iam:CreateRole",
                "iam:CreatePolicy",
                "ec2:CreateVpc",
                "ec2:CreateSubnet",
                "ec2:CreateSecurityGroup",
                "ec2:RunInstances",
                "rds:CreateDBInstance",
                "lambda:CreateFunction",
                "apigateway:*"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}